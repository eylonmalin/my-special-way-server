{
  "rootDir": ".",
  "testRegex": ".e2e-spec.ts$",
  "coverageDirectory": "../coverage",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleDirectories": [
    "node_modules",
    "../node_modules"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!src/main.ts",
    "!src/main.hmr.ts",
    "!src/**/*.module.ts",
    "!src/**/*.spec.ts",
    "temporary_disabling_coverage_until_feature_is_done",
    "!src/mock_temporary/*.*",
    "!src/modules/graphql/queries/*.*",
    "!src/modules/graphql/types/*.*",
    "!src/**/*.temp.ts"
  ]
}